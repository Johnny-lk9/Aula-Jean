Algoritmo "exercicio10"

Var

   altura, raio, volume : real

Inicio

   escreva("Digite o valor do raio do cilindro: ")
   leia(raio)

   escreva("Digite o valor da altura do cilindro: ")
   leia(altura)
   
   volume <- Pi * Exp(raio, 2) * altura
   
   escreva("O volume do cilindro e: ", volume)
   
Fimalgoritmo